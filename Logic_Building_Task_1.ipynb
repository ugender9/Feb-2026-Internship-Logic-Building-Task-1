print("="*50)
print("Task 2: Pass / Fail Analyzer")
print("="*50)

marks = [45, 78, 90, 33, 60]

pass_count = 0
fail_count = 0

for i, mark in enumerate(marks, start=1):
    if mark >= 50:
        pass_count += 1
        status = "Pass"
    else:
        fail_count += 1
        status = "Fail"
    print(f"Student {i}: {mark} marks - {status}")

print("\n" + "="*50)
print("Final Results:")
print(f"Total Students: {len(marks)}")
print(f"Pass Students: {pass_count}")
print(f"Fail Students: {fail_count}")
